<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nam.dtapp">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.activity.start.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.intro.MyIntro"></activity>
        <activity android:name=".view.activity.home.SearchActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".view.activity.start.RegisterActivity" />
        <activity android:name=".view.activity.start.RegisterManFieldActivity"></activity>
        <activity android:name=".view.activity.home.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".view.activity.comment.CommentActivity"></activity>
        <activity android:name=".view.activity.newfeed.UpLoadActivity"></activity>
        <activity android:name=".view.activity.profile.ProfileActivity"
            android:parentActivityName=".view.activity.home.HomeActivity"></activity>
        <activity android:name=".view.activity.profile.UpdateProfileActivity"
            android:parentActivityName=".view.activity.profile.ProfileActivity"></activity>
        <activity android:name=".view.activity.start.RegisterConcernIDActivity"></activity>
        <activity android:name=".view.activity.newfeed.Acivity_tag"></activity>


    </application>

</manifest>